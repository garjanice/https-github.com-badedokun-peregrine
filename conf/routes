# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page

GET     /                           com.depth1.grc.controllers.Application.index()

GET 	/riskRegister/create/			com.depth1.grc.controllers.Application.showCreateRRPage()

GET 	/riskRegister/view/			com.depth1.grc.controllers.Application.showViewRRPage()

GET 	/riskRegister/update/			com.depth1.grc.controllers.Application.showUpdateRRPage()

GET 	/riskRegister/delete/			com.depth1.grc.controllers.Application.showDeleteRRPage()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
